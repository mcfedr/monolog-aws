{
    "name": "mcfedr/monolog-aws",
    "description": "A monolog handler to send logs to SNS or SES",
    "keywords": ["log", "monolog", "amazon", "aws", "sns", "ses"],
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "aws/aws-sdk-php": "^3",
        "monolog/monolog": "^1.13 || ^2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^8",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {"Mcfedr\\Monolog\\": "src/Mcfedr/Monolog"}
    },
    "autoload-dev": {
        "psr-4": {"Mcfedr\\Monolog\\": "tests/Mcfedr/Monolog"}
    }
}
